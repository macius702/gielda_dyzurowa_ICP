{
  "canisters": {
    "d_backend": {
      "candid": "src/d_backend/d_backend.did",
      "package": "d_backend",
      "type": "custom",
      "build": "./build.sh d_backend",
      "wasm": "target/wasm32-unknown-unknown/release/d_backend.wasm"
    },
    "d_frontend": {
      "dependencies": [
        "d_backend"
      ],
			"frontend": {
				"entrypoint": "src/d_frontend/build/web/index.html"
			},
      "source": [
        "src/d_frontend/build/web"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
			"packtool": ""
    }
  },
	"networks": {
		"local": {
			"bind": "127.0.0.1:4944",
			"type": "ephemeral"
		}
	},  
  "output_env_file": ".env",
  "version": 1
}